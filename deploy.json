{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {},
  "items": [
    {
      "apiVersion": "v1",
      "kind": "Namespace",
      "metadata": {
        "name": "app-dev",
        "labels": {
          "name": "app-dev"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "aks-service-test",
        "namespace": "app-dev"
      },
      "spec": {
        "type": "LoadBalancer",
        "ports": [
          {
            "port": 80,
            "targetPort": 80
          }
        ],
        "selector": {
          "app": "aks"
        }
      }
    },
    {
      "kind": "Deployment",
      "apiVersion": "apps/v1",
      "metadata": {
        "name": "aks.testing",
        "namespace": "app-dev",
        "labels": {
          "app": "aks"
        }
      },
      "spec": {
        "replicas": 2,
        "selector": {
          "matchLabels": {
            "app": "aks"
          }
        },
        "template": {
          "metadata": {
            "labels": {
              "app": "aks"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "aks-testing",
                "image": "mabusaa/aksdevspaces:stable",
                "imagePullPolicy": "Always",
                "ports": [
                  {
                    "containerPort": 80
                  }
                ],
                "env": [
                  {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "Development"
                  },
                  {
                    "name": "ASPNETCORE_URLS",
                    "value": "http://0.0.0.0:80"
                  }
                ],
                "resources": {
                  "limits": {
                    "memory": "200Mi"
                  },
                  "requests": {
                    "memory": "200Mi"
                  }
                }
              }
            ],
            "restartPolicy": "Always"
          }
        },
        "strategy": {}
      },
      "status": {}
    }
  ]
}
